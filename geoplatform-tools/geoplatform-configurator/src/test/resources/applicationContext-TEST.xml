<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
            http://www.springframework.org/schema/beans/spring-beans-3.1.xsd"
       default-autowire="byName">
    
    <bean id="gpDigesterSHA1" class="org.geosdi.geoplatform.configurator.crypt.GPDigester">
        <property name="algorithm" value="SHA-1" />
    </bean>
    
    <bean id="gpDigesterMD5" class="org.geosdi.geoplatform.configurator.crypt.GPDigester">
        <property name="algorithm" value="MD5" />
    </bean>

</beans>
