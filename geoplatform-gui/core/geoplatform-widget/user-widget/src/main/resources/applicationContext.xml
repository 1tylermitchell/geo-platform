<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd"
	default-autowire="byName">

    <context:annotation-config />

    <import resource="applicationContext-Toolbar.xml" />

    <bean name="manageUsers"
        class="org.geosdi.geoplatform.gui.configuration.menubar.MenuBarClientTool">
        <property name="id" value="manageUsers" />
        <property name="text" value="Manage Users" />
        <property name="enabled" value="true" />
        <property name="order" value="10" />        
    </bean>

</beans>